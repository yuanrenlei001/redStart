<view class="main">
  <view class="mainLeft">
    <view a:for="{{listLeft}}" data-id="{{item.dictValue}}" onTap="sortActive" class="{{item.dictValue == typeId?'active':''}}">{{item.dictLabel}}</view>
  </view>
  <view class="mainRight">
    <navigator url="/pages/redHome/redImg/redImg" a:for="{{listRight}}">
      <view class="title">{{item.name}}</view>
      <view class="time">{{item.createTime}}</view>
      <view class="see"><image mode="scaleToFill" src="/images/index/see.png"/> 10</view>
    </navigator>
  </view>
</view>